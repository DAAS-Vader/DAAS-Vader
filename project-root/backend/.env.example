# Application Configuration
PORT=3000
NODE_ENV=development

# Database Configuration (현재 사용하지 않음)
# DATABASE_URL=postgres://user:pass@localhost:5432/daas

# Authentication
DEV_ADMIN_TOKEN=dev-allow
SESSION_SECRET=change-me-32bytes-long-secret-key

# Seal Service Configuration
SEAL_URL=https://seal.example.com
SEAL_SERVICE_TOKEN=server-token-or-oidc
SEAL_TICKET_SECRET=shared-hs256-secret-for-tickets

# Seal v2 Configuration (Move Contracts)
SEAL_PACKAGE_ID=0x5c5766dbaeaa274325c4c37f63a02c3541860f630327f6aaef290a0c1c4fdcf2
SEAL_REGISTRY_ID=0x...
ENCLAVE_REGISTRY_ID=0x...
ADMIN_PRIVATE_KEY=hex-encoded-private-key
DEFAULT_IDENTITY=hex-encoded-default-identity

# Key Server Configuration
KEY_SERVER_MODE=open
KEY_SERVER_ADMIN_KEY=hex-encoded-admin-private-key
KEY_ROTATION_ENABLED=true
KEY_ROTATION_INTERVAL=86400000
KEY_MAX_AGE=604800000
KEY_PRE_ROTATION_PERIOD=3600000
KEY_RETENTION_PERIOD=300000
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000
RATE_LIMIT_BURST=10
MAX_CONCURRENT_KEYS=100
KEY_ENCRYPTION_KEY=your-master-key-for-key-encryption
KEY_BACKUP_ENABLED=true
KEY_AUDIT_ENABLED=true

# Sui Network Configuration
SUI_RPC_URL=https://fullnode.testnet.sui.io:443
SUI_NETWORK=testnet

# Monitoring Configuration
METRICS_RETENTION_MS=86400000
ALERT_RETENTION_MS=604800000
HEALTH_CHECK_INTERVAL_MS=30000
METRICS_AGGREGATION_INTERVAL_MS=300000

# Monitoring Thresholds
KEY_SERVER_ERROR_RATE_THRESHOLD=0.05
KEY_SERVER_RESPONSE_TIME_THRESHOLD=5000
SEAL_SERVICE_ERROR_RATE_THRESHOLD=0.1
SEAL_SERVICE_RESPONSE_TIME_THRESHOLD=10000
SUI_NETWORK_RESPONSE_TIME_THRESHOLD=3000

# Alert Configuration
ALERT_WEBHOOK_URL=https://hooks.example.com/alerts
ALERT_EMAIL_SMTP=smtp.gmail.com:587
ALERT_EMAIL_USER=alerts@yourdomain.com
ALERT_EMAIL_PASSWORD=your-email-password
ALERT_EMAIL_TO=admin@yourdomain.com,ops@yourdomain.com
ALERT_SLACK_WEBHOOK=https://hooks.slack.com/services/...
ALERT_SLACK_CHANNEL=#alerts

# Walrus Configuration (Official Testnet)
WALRUS_PUBLISHER=https://publisher.walrus-testnet.walrus.space
WALRUS_AGGREGATOR=https://aggregator.walrus-testnet.walrus.space

# Walrus SDK Configuration (Optional - for enhanced features)
WALRUS_NETWORK=testnet
WALRUS_KEYPAIR_SEED=your-mnemonic-phrase-here
WALRUS_WAL_COIN_TYPE=0x...::wal::WAL
USE_WALRUS_SDK=true

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-12-digit-aws-account-id

# Nautilus TEE Configuration
NAUTILUS_API_KEY=your-nautilus-api-key-here
NAUTILUS_ENDPOINT=http://localhost:8080
ENABLE_NAUTILUS=false

# TEE Security Configuration
TEE_SECURITY_LEVEL=medium
TEE_ENABLE_ATTESTATION=true
TEE_ENABLE_PCR_VALIDATION=true
TEE_ENABLE_IDENTITY_VERIFICATION=true
TEE_ATTESTATION_KEY=your-tee-attestation-private-key
ENCLAVE_MASTER_KEY=your-64-char-hex-enclave-master-private-key

# Docker Builder Configuration
DOCKER_BUILDER_ENDPOINT=http://localhost:3001
DOCKER_BUILD_DIR=/tmp/docker-builds
MAX_CONCURRENT_BUILDS=3
BUILD_TIMEOUT=600

# GitHub App Configuration (Optional)
GITHUB_APP_ID=123456
GITHUB_APP_PRIVATE_KEY_PEM="-----BEGIN PRIVATE KEY-----
MIIEvQIBADANBgkqhkiG9w0BAQEFAASCBKcwggSjAgEAAoIBAQC...
-----END PRIVATE KEY-----"
GITHUB_APP_WEBHOOK_SECRET=change-me-webhook-secret
GITHUB_APP_SLUG=my-daas-app

# File Processing Limits
MAX_SECRET_FILE_SIZE=10485760
MAX_SECRET_BUNDLE_SIZE=20971520
MAX_CODE_BUNDLE_SIZE=209715200
REQUEST_TIMEOUT=180000