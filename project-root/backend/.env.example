# Application Configuration
PORT=3000
NODE_ENV=development

# Database Configuration (현재 사용하지 않음)
# DATABASE_URL=postgres://user:pass@localhost:5432/daas

# Authentication
DEV_ADMIN_TOKEN=dev-allow
SESSION_SECRET=change-me-32bytes-long-secret-key

# Seal Service Configuration
SEAL_URL=https://seal.example.com
SEAL_SERVICE_TOKEN=server-token-or-oidc
SEAL_TICKET_SECRET=shared-hs256-secret-for-tickets

# Walrus Configuration (Official Testnet)
WALRUS_PUBLISHER=https://publisher.walrus-testnet.walrus.space
WALRUS_AGGREGATOR=https://aggregator.walrus-testnet.walrus.space

# Walrus SDK Configuration (Optional - for enhanced features)
WALRUS_NETWORK=testnet
WALRUS_KEYPAIR_SEED=your-mnemonic-phrase-here
WALRUS_WAL_COIN_TYPE=0x...::wal::WAL
USE_WALRUS_SDK=true

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=your-12-digit-aws-account-id

# Nautilus TEE Configuration
NAUTILUS_API_KEY=your-nautilus-api-key-here
NAUTILUS_ENDPOINT=http://localhost:8080
ENABLE_NAUTILUS=false

# Docker Builder Configuration
DOCKER_BUILDER_ENDPOINT=http://localhost:3001
DOCKER_BUILD_DIR=/tmp/docker-builds
MAX_CONCURRENT_BUILDS=3
BUILD_TIMEOUT=600

# GitHub App Configuration (Optional)
GITHUB_APP_ID=123456
GITHUB_APP_PRIVATE_KEY_PEM="-----BEGIN PRIVATE KEY-----
MIIEvQIBADANBgkqhkiG9w0BAQEFAASCBKcwggSjAgEAAoIBAQC...
-----END PRIVATE KEY-----"
GITHUB_APP_WEBHOOK_SECRET=change-me-webhook-secret
GITHUB_APP_SLUG=my-daas-app

# File Processing Limits
MAX_SECRET_FILE_SIZE=10485760
MAX_SECRET_BUNDLE_SIZE=20971520
MAX_CODE_BUNDLE_SIZE=209715200
REQUEST_TIMEOUT=180000